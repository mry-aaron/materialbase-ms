<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <!-- 设置开启映射下划线 -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <!-- 设置别名 -->
    <typeAliases>
        <typeAlias alias="UserBean" type="net.qicp.aaron.domain.UserBean" />
        <typeAlias alias="DeptBean" type="net.qicp.aaron.domain.DeptBean" />
    </typeAliases>

</configuration>